<Bindings>
	<Binding name="ARKINV_CONFIG" header="ARKINV" category="ADDONS">
		ArkInventory.Frame_Config_Toggle( )
	</Binding>
	<Binding name="ARKINV_TOGGLE_BAG" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Bag )
	</Binding>
	<Binding name="ARKINV_TOGGLE_BANK" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Bank )
	</Binding>
	<Binding name="ARKINV_TOGGLE_VAULT" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Vault )
	</Binding>
	<Binding name="ARKINV_TOGGLE_VOID" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Void )
	</Binding>
	<Binding name="ARKINV_TOGGLE_MAIL" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Mail )
	</Binding>
	<Binding name="ARKINV_TOGGLE_WEARING" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Wearing )
	</Binding>
	<Binding name="ARKINV_TOGGLE_PET" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Pet )
	</Binding>
	<Binding name="ARKINV_TOGGLE_MOUNT" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Mount )
	</Binding>
	<Binding name="ARKINV_TOGGLE_TOYBOX" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Toybox )
	</Binding>
	<Binding name="ARKINV_TOGGLE_HEIRLOOM" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Heirloom )
	</Binding>
	<Binding name="ARKINV_TOGGLE_TOKEN" category="ADDONS">
		ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Token )
	</Binding>
	<Binding name="ARKINV_TOGGLE_EDIT" category="ADDONS">
		ArkInventory.Frame_Main_Show( ArkInventory.Const.Location.Bag )
		ArkInventory.ToggleEditMode( )
	</Binding>
	<Binding name="ARKINV_REFRESH" category="ADDONS">
		ArkInventory.Frame_Main_Generate( nil, ArkInventory.Const.Window.Draw.Resort )
	</Binding>
	<Binding name="ARKINV_RELOAD" category="ADDONS">
		ArkInventory.ItemCacheClear( )
		ArkInventory.Frame_Main_Generate( nil, ArkInventory.Const.Window.Draw.Recalculate )
	</Binding>
	<Binding name="ARKINV_RESTACK" category="ADDONS">
		ArkInventory.Restack( ArkInventory.Const.Location.Bag )
	</Binding>
	<Binding name="ARKINV_TOGGLE_RULES" category="ADDONS">
		ArkInventory.Frame_Rules_Toggle( )
	</Binding>
	<Binding name="ARKINV_TOGGLE_SEARCH" category="ADDONS">
		ArkInventory.Frame_Search_Toggle( )
	</Binding>
	<Binding name="ARKINV_LDB_PETS_SUMMON" category="ADDONS">
		ArkInventory.LDB.Pets.OnClick( )
	</Binding>
	<Binding name="CLICK ARKINV_MountToggle:LeftButton" category="ADDONS" />
</Bindings>
