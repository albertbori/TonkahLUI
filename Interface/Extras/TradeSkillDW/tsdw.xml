<Ui>
	<CheckButton name="TradeSkillDW_SkillTabTemplate" inherits="SecureActionButtonTemplate" virtual="true">
		<Size x="32" y="32"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBackground" file="Interface\SpellBook\SpellBook-SkillLineTab">
					<!-- rotate 90 todo ? -->
					<Size x="64" y="64"/>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset x="-3" y="11"/>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Cooldown name="$parentCooldown" inherits="CooldownFrameTemplate" parentKey="cooldown">
				<Size x="36" y="36"/>
				<Anchors>
					<Anchor point="CENTER">
						<Offset>
							<AbsDimension x="0" y="-1"/>
						</Offset>
					</Anchor>
				</Anchors> 
			</Cooldown>
		</Frames>
		<Scripts>
			<OnLoad>
				self:SetAttribute("type", "macro")
			</OnLoad>
			<OnEnter>
				TradeSkillDW_SkillTab_OnEnter(self)
			</OnEnter>
			<OnLeave>
				TradeSkillDW_SkillTab_OnLeave(self)
			</OnLeave>
		</Scripts>
		<NormalTexture/>
		<HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD"/>
		<CheckedTexture file="Interface\Buttons\CheckButtonHilight" alphaMode="ADD"/>
	</CheckButton>

